		init : function(){
			if(this.goods_data.length==0)
				return;

			//选中当前商品规格
			//this.current_goods = this.goods_data[this.goods_id];






            for(var i=0;i<this.goods_data.length;i++){
				if(this.goods_data[i].goods_id == buy_object.goods_id){
					this.current_goods = this.goods_data[i];
					$('span[sk='+this.goods_data[i].spec_index+']').attr('class','xq-body-2-b-4-select');
				}
			}


			//calc_discount();

			$(".xq-body-2-b-4 span").click(function(){
				$(this).addClass('xq-body-2-b-4-select').siblings().removeClass('xq-body-2-b-4-select');
				buy_object.change_spec();
				//calc_discount();
			});

			//数量 增加
			$("span[alt=1]").click(function(){

				if(isNaN($("#bug_num_input").val())){
					$("#bug_num_input").val = 1;
				}

				var n = parseInt($("#bug_num_input").val());
				n++;
				n = (n<=1) ? 1 : n;
                //var limit_num = buy_object.current_goods.limit_num;
                var limit_num =parseInt(buy_object.limit_num);
                //var storage =parseInt(buy_object.goods_storage);//这里会不会有问题
                var storage = buy_object.current_goods.goods_storage;//改成这样了

                if(limit_num!=0){
                    if(n > limit_num){
                        n=limit_num;
                        alert('超过限购数量');
                    }
                }
                if(n > storage){
                    n=storage;
                    alert('库存不足');
                }
				buy_object.buy_num = n;
				buy_object.display_htm();//前端重新渲染 是为了改 buy_object.buy_num = n
				//calc_discount();
				$("#bug_num_input").val(n);

			});
			//数量 减少
			$("span[alt=-1]").click(function(){

				if(isNaN($("#bug_num_input").val())){
					$("#bug_num_input").val(1);
				}

				var n = parseInt($("#bug_num_input").val());
				n--;
				n = (n<=1) ? 1 : n;
				buy_object.buy_num = n;
				buy_object.display_htm();//前端重新渲染 是为了改 buy_object.buy_num = n
				//calc_discount();
				$("#bug_num_input").val(n);

			});

			$("#bug_num_input").blur(function(){
				if(isNaN($("#bug_num_input").val())){
					$("#bug_num_input").val(1);
					buy_object.buy_num = 1;
				}
				//calc_discount();
			});

			//提交购买
			$("#buy_submit_but").click(function(){
				buy_object.buy_submit();
			});

			//加入购物车
			$("#add_cart_but").click(buy_object.add_cart_request);
			$("#close_addcartsuccess_dialog_but").click(buy_object.close_addcartsuccess_dialog);
			$("#keep_shopping_but").click(buy_object.close_addcartsuccess_dialog);

		},
