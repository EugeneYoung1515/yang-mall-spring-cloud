	function calc_discount(){
		var discount = 0;
		var price = parseFloat($.trim($('.new_price_box strong').html().substring(1)));
		var origin_price = parseFloat($.trim($('.new_price_box span').html()).substring(1));
		discount = (price/origin_price*10).toFixed(1)+'折';
		$('.new_price_box em').eq(0).html(discount);
	}

